local Main = MakeTab({Name = "auto farm"})


MakeNotifi({
  Title = "made by kronw_",
  Text = "(gabitu is gay)",
  Time = 10
})
local Toggle = AddToggle(Main, {
  Name = "Toggle teste",
  Default = false,
  Callback = function(Value)
    while getenv().AutoFarmCoins == true do
    
    
    
    local function AutoFarmCoins()
  local ReplicatedStorage = game:GetService("ReplicatedStorage")
  local Players = game:GetService("Players")
  local Player = Players.LocalPlayer
  
  local Network = ReplicatedStorage:WaitForChild("Network", 9e9)
  local THINGS = workspace:WaitForChild("__THINGS", 9e9)
  
  local function GetCoins()
    local coins = {}
    for _,coin in pairs(THINGS.Breakables:GetChildren()) do
      local plrPP = Player.Character and Player.Character.PrimaryPart
      
      if coin.Name ~= "Highlight" and plrPP and coin.PrimaryPart then
        if (plrPP.Position - coin.PrimaryPart.Position).Magnitude < getgenv().AutoFarmDistance then
          table.insert(coins, coin)
        end
      end
    end
    return coins
  end
  
  while getgenv().AutoFarmCoins do task.wait()
    local Coin = GetCoins()[math.random(1, #GetCoins())]
    
    if Coin then
      Network.Breakables_PlayerDealDamage:FireServer(Coin.Name)
        end
      end
     end


getgenv().AutoFarmDistance = 70
getgenv().AutoFarmCoins = true
AutoFarmCoins()
    
    end
  end
})




AddButton(Main, {
  Name = "auto collect orbs/lootbags",
  Callback = function()
    
    
    local player = game.Players.LocalPlayer
local character = player.Character
local humanoidRootPart = character and character:WaitForChild("HumanoidRootPart")
local things = workspace.__THINGS

local function moveObjects()
    if humanoidRootPart then
        local playerCFrame = humanoidRootPart.CFrame

        for i, v in pairs(things.Lootbags:GetChildren()) do
            v:PivotTo(playerCFrame)
        end

        for _, orb in ipairs(things.Orbs:GetChildren()) do
            if orb:IsA("BasePart") then
                orb.Transparency = 1 -- Make the orb invisible
                orb.CanCollide = false -- Disable collision during teleportation
                orb.CFrame = playerCFrame
                -- You can optionally destroy the orb after teleportation if desired
            end
        end
    end
end

-- Assuming the collection process is handled elsewhere in the game,
-- let's add a function to handle the collected orbs

local function handleCollectedOrbs()
    for _, orb in ipairs(things.Orbs:GetChildren()) do
        if orb:IsA("BasePart") and orb.Transparency == 1 then
            orb:Destroy() -- Destroy the collected orb
            -- Alternatively, you can move the orb to a different location far from the player's sight
            -- orb.CFrame = CFrame.new(Vector3.new(0, -1000, 0)) -- Move the orb to a distant location
        end
    end
end

-- Continuously move objects and handle collected orbs
while true do
    moveObjects()
    handleCollectedOrbs()
    wait(0.1)
end

    
    
  end
})

